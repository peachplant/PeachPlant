<h1>Users#show</h1>
<p>Find me in app/views/users/show.html.erb</p>

<p>User: <%= @user.email %></p>
<p>First name: <%= @user.first_name %></p>

<h2>My Listings</h2>

<div id="search-results">
  <ul>
    <% @user.listings.each do |listing| %>
        <%=  render listing %>
    <% end %>
  </ul>
</div>

